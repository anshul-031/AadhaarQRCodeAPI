{
  "builds": [
    {
      "src": "pages/api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "scripts/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "pages/api/**/*.ts": {
      "runtime": "nodejs16.x"
    },
    "scripts/**/*.py": {
      "runtime": "python3.9"
    }
  }
}
